[Unit]
Description=Gunicorn instance to serve Airbnb clone
After=network.target

[Service]
User=ubuntu
Group=ubuntu
WorkingDirectory=/home/ubuntu/AirBnB_clone_v4
ExecStart=/usr/local/bin/gunicorn --workers 3 --bind unix:/home/ubuntu/AirBnB_clone_v4/gunicorn.sock web_dynamic.2-hbnb:app

[Install]
WantedBy=multi-user.target
